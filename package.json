{
  "name": "voipms-monitor",
  "version": "1.0.0",
  "description": "Monitor voip.ms registration status",
  "main": "built/index.js",
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "compile": "tsc",
    "build": "npm run lint && npm run compile",
    "preparepackage": "mkdir -p packaged && cp -r *.yml *.json built packaged && cd packaged && npm install --production",
    "package": "gulp --gulpfile built/gulpfile.js package",
    "test": "npm run build && npm run preparepackage && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsutclif/voipms-monitor.git"
  },
  "author": "Nathan Sutcliffe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nsutclif/voipms-monitor/issues"
  },
  "homepage": "https://github.com/nsutclif/voipms-monitor#readme",
  "dependencies": {
    "chai": "4.1.2",
    "request": "2.82.0",
    "request-promise-native": "1.0.5"
  },
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/aws-lambda": "0.0.16",
    "@types/gulp": "^4.0.4",
    "@types/js-yaml": "3.9.1",
    "@types/node": "7.0.29",
    "@types/request": "2.0.3",
    "@types/request-promise-native": "1.0.7",
    "@types/vinyl": "2.0.1",
    "aws-sdk": "2.124.0",
    "gulp": "3.9.1",
    "gulp-modify-file": "1.0.0",
    "gulp-s3-upload": "1.6.6",
    "gulp-zip": "4.0.0",
    "js-yaml": "3.10.0",
    "tslint": "5.7.0",
    "typescript": "2.5.3",
    "vinyl": "2.1.0"
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ]
  }
}
